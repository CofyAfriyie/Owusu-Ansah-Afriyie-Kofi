# include <iostream>
using namespace std;

int gcd(int a, int b)
{
 return (b!=0)? gcd (b,a%b): a;
}

int main ()
{
 int a,b,answer;
cout<< "Enter first number:";
cin>> a;
cout<< "Enter second number:";
cin>> b;
answer= gcd(a,b);
if(gcd)
 cout << "The GCD of " << a << " and " << b << " is " << answer <<endl;
else
cout<< "\nInva;id input!!!\n";
return 0;
}
